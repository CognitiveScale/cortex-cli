  "author": "CognitiveScale",
  "license": "Apache-2.0",
  "homepage": "https://github.com/CognitiveScale/cortex-cli",
  "dependencies": {
    "@c12e/generator-cortex": "0.3.30",
    "@hapi/joi": "17.1.1",
    "boxen": "4.2.0",
    "chalk": "3.0.0",
    "cli-table": "0.3.1",
    "co": "4.6.0",
    "co-prompt": "1.0.0",
    "commander": "5.0.0",
    "debug": "4.1.1",
    "event-stream": "4.0.1",
    "find-package-json": "1.2.0",
    "glob": "7.1.6",
