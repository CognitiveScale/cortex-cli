{
  "name": "cortex-cli",
  "version": "2.2.0",
  "description": "Command line tool for CognitiveScale Cortex.",
  "main": "./bin/cortex.js",
  "type": "module",
  "types": "./client/index.d.ts",
  "scripts": {
    "build-binaries": "pkg .",
    "prepare": "is-ci || husky install",
    "prepack": "cp package-lock.json npm-shrinkwrap.json",
    "postpack": "rm npm-shrinkwrap.json",
    "pretest": "eslint bin/**/*.js src/**/*.js test/**/*.js && audit-ci -h",
    "test": "cross-env NODE_ENV=test c8 mocha",
    "test:workspaces": "cross-env NODE_ENV=test mocha --grep='Workspaces'",
    "clean": "rm -rf dist/ coverage/ .nyc_output/ node_modules/ cortex-cli.tgz cortex-cli-docs.md version.txt"
  },
  "pkg": {
    "outputPath": "dist",
    "targets": [
      "node16-win-x64",
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-macos-arm64"
    ],
    "assets": [
      "node_modules/open/xdg-open"
    ]
  },
  "files": [
    "bin/*",
    "src/*",
    "client/*",
    "README.md",
    "npm-shrinkwrap.json",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CognitiveScale/cortex-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cognitivescale",
    "cortex",
    "ai"
  ],
  "bin": {
    "cortex": "./bin/cortex.js"
  },
  "author": "CognitiveScale",
  "license": "Apache-2.0",
  "homepage": "https://github.com/CognitiveScale/cortex-cli",
  "dependencies": {
    "@supercharge/promise-pool": "2.4.0",
    "boolean": "3.2.0",
    "chalk": "5.2.0",
    "cli-progress": "3.12.0",
    "cli-table3": "0.6.3",
    "commander": "10.0.0",
    "dayjs": "1.11.7",
    "debug": "4.3.4",
    "dockerode": "3.3.4",
    "es-main": "1.2.0",
    "form-data": "4.0.0",
    "get-stream": "6.0.1",
    "gh-got": "10.0.0",
    "glob": "9.2.1",
    "got": "12.6.0",
    "graphql": "16.6.0",
    "graphql-request": "5.2.0",
    "inquirer": "9.1.4",
    "is-installed-globally": "0.4.0",
    "istextorbinary": "6.0.0",
    "jmespath": "0.16.0",
    "joi": "17.8.3",
    "jose": "4.13.1",
    "js-yaml": "4.1.0",
    "keytar": "7.9.0",
    "lodash": "4.17.21",
    "minimatch": "7.4.2",
    "npm-registry-fetch": "14.0.3",
    "open": "8.4.2",
    "prompts": "2.4.2",
    "semver": "7.3.8",
    "treeify": "1.1.0",
    "url-join": "5.0.0",
    "url-parse": "1.5.10",
    "uuid": "9.0.0",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-syntax-import-assertions": "7.20.0",
    "audit-ci": "6.6.1",
    "c8": "7.13.0",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "cross-env": "7.0.3",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "mocha": "10.2.0",
    "mocked-env": "1.3.5",
    "nock": "13.3.0",
    "pkg": "5.8.1",
    "sinon": "15.0.1"
  },
  "c8": {
    "check-coverage": true,
    "statements": 40,
    "branches": 72,
    "functions": 17,
    "lines": 40,
    "include": [
      "bin",
      "src"
    ],
    "reporter": [
      "text",
      "html"
    ]
  }
}
