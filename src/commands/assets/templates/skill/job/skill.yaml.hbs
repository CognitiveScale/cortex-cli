camel: 1.0.0
name: c12e/{{dashCase name}}
title: {{name}}
description: {{name}}
inputs:
  - name: params
    title: Job Input
    parameters:
      - name: params
        type: object
        description: Input Params
        required: true
    routing:
      all:
        action  : c12e/{{dashCase name}}
        runtime: cortex/jobs
        output: response

outputs:
  - name: result
    title: Job result
    parameters:
      - name: result
        type: object
        description: Job Execution Logs
        required: true
